{
  "name": "embev-api",
  "version": "0.1.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "check:pack": "npx depcheck",
    "check:prune": "npm prune --production && npm fund",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug:default": "nest start --debug --watch",
    "start:debug": "cross-env PORT=3077 nest start --debug 9222 --watch",
    "start:local": "dotenv -e .env.local -e .env -- nest start --debug 9222 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "docker:compose:db": "sudo docker compose up -d",
    "migration:init": "mikro-orm schema:update --run --config ./src/config/database/typeorm.config.ts",
    "migration:init:empty": "mikro-orm schema:refresh --run --config ./src/config/database/typeorm.config.ts",
    "migration:create": "mikro-orm migration:create --config ./src/config/database/typeorm.config.ts",
    "migration:create:init": "mikro-orm migration:create --initial --config ./src/config/database/typeorm.config.ts",
    "migration:create:init:npx": "npx mikro-orm migration:create --initial --config ./src/config/database/typeorm.config.ts",
    "migration:down": "mikro-orm migration:down --config ./src/config/database/typeorm.config.ts",
    "migration:up": "mikro-orm migration:up --config ./src/config/database/typeorm.config.ts"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.4.13",
